<p id="notice"><%= notice %></p>

<h1>House Hunters</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Contact method</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @house_hunters.each do |house_hunter| %>
      <tr>
        <td><%= house_hunter.email %></td>
        <td><%= house_hunter.password %></td>
        <td><%= house_hunter.name %></td>
        <td><%= house_hunter.phone %></td>
        <td><%= house_hunter.contact_method %></td>
        <td><%= link_to 'Show', house_hunter %></td>
        <td><%= link_to 'Edit', edit_house_hunter_path(house_hunter) %></td>
        <td><%= link_to 'Destroy', house_hunter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


